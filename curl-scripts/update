curl --include --request PATCH "https://tic-tac-toe-wdi.herokuapp.com/games/" \
  --header "Authorization: Token token=${TOKEN}" \
  --header "Content-Type: application/json" \
  --data '{
    "game": {
      "cell": {
      "index": "'"${INDEX}"'",
      "value": "'"${VALUE}"'"
    }
    "over": "'"${BOOLEAN}"'"
  }'

echo
