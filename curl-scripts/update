curl "https://tic-tac-toe-wdi.herokuapp.com/games/3475" \
  --include \
  --request PATCH \
  --header "Authorization: Token token=${TOKEN}" \
  --header "Content-Type: application/json" \
  --data '{
    "game": {
      "cells": {
      "index": "'"${INDEX}"'",
      "value": "'"${VALUE}"'"
    }
    "over": "'"${BOOLEAN}"'"
  }'

echo
